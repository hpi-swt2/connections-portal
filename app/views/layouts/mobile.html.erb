<!DOCTYPE html>
<html>

<head>
  <%= render 'layouts/shared_head' %>
  <%= stylesheet_link_tag 'mobile', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="container pt-3">
    <% if notice %><div class="alert alert-success notice" role="alert"><%= notice %></div><% end %>
    <% if alert %><div class="alert alert-danger" role="alert"><%= alert %></div><% end %>
    <%= yield %>
  </div>
</body>

</html>
