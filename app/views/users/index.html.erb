<h1>Users</h1>
<table>
  <thead>
  <tr>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= button_to '+', user_contact_requests_path(user), method: :post, id: user.id %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
