<%= stylesheet_link_tag 'users', media: 'all', 'data-turbolinks-track': 'reload' %>
<% if user.avatar.attached? %>
  <%= image_tag user.avatar, height: size, width: size, class: "#{classes.join(' ')}" %>
<% else %>
  <img class="<%= classes.join(' ') %>" height="<%= size %>" width="<%= size %>"
       src="https://user-images.githubusercontent.com/48409110/107123576-8aef6b80-689e-11eb-9386-16eecd0ceb9e.png">
<% end %>
